[sub_resource type="GDScript" id=49]
script/source = "extends StaticBody2D
var player_on = false
var t = 1

func cerp(a,b,t): 
	var f = (1-cos(t*PI))*.5 
	return a*(1-f)+b*f 

func _process(delta):
	if player_on:
		$dialog.rect_scale.x = cerp($dialog.rect_scale.x, 1, 13*delta)
	else:
		$dialog.rect_scale.x = cerp($dialog.rect_scale.x, 0, 13*delta)
		
	$dialog.visible = ($dialog.rect_scale.x>0.05)


func _on_detector_body_entered(body):
	if body.get_name()==\"hyenakid\":
		player_on = true

func _on_detector_body_exited(body):
	if body.get_name()==\"hyenakid\":
		player_on = false
"

